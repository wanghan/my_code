<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="dao.hibernate">
	<class
		name="DbPaper"
		table="db_paper"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="Id"
			type="integer"
			column="id"
		>
			<generator class="native"/>
		</id>

		<property
			name="Title"
			column="title"
			type="string"
			not-null="true"
			length="255"
		/>
		<property
			name="Pages"
			column="pages"
			type="string"
			not-null="false"
			length="255"
		/>
		<property
			name="Source"
			column="source"
			type="string"
			not-null="false"
			length="255"
		/>
		<property
			name="Link"
			column="link"
			type="string"
			not-null="false"
			length="255"
		/>
		<property
			name="Abstract"
			column="abstract"
			type="string"
			not-null="true"
		/>
		<property
			name="Doi"
			column="doi"
			type="string"
			not-null="false"
			length="255"
		/>
		<property
			name="DoiLink"
			column="doi_link"
			type="string"
			not-null="false"
			length="255"
		/>
		<property
			name="TmIndex"
			column="tm_index"
			type="integer"
			not-null="true"
			length="10"
		/>
		<many-to-one
			name="Conference"
			column="conference_id"
			class="DbConference"
			not-null="true"
		>
		</many-to-one>
	
		<set
			name="DbAuthors"
			table="db_paper_authors"
			cascade="all"
		>
			<key column="paper_id"/>
			<many-to-many column="author_id" class="DbAuthor"/>
		</set>


	</class>	
</hibernate-mapping>